        <pgterms:name>Albertus, Magnus, Saint</pgterms:name>
        <pgterms:deathdate rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1280</pgterms:deathdate>
        <pgterms:alias>Albert, the Great, Saint</pgterms:alias>
